{
    "name": "@EarthW0rm/royal-gorilla",
    "version": "1.0.0",
    "scripts": {
        "gulp": "node ./node_modules/gulp/bin/gulp",
        "tslint": "node ./node_modules/tslint/bin/tslint",
        "eslint": "node ./node_modules/eslint/bin/eslint",
        "gulp-task-test": "yarpm run gulp test",
        "gulp-task-build": "yarpm run gulp build",
        "gulp-task-developer": "yarpm run gulp developer",
        "lint": "yarpm run tslint -c src-server/tslint.json 'src-server/**/*.ts' && yarpm run eslint ./src-front/**/*.js ./src-front/**/*.jsx",
        "test": "yarpm run lint && cross-env NODE_ENV=development DEV_ATTACH=false yarpm run gulp-task-test",
        "start": "yarpm run lint && cross-env NODE_ENV=development DEV_ATTACH=false yarpm run gulp-task-developer",
        "debug": "cross-env NODE_ENV=development DEV_ATTACH=true yarpm run gulp-task-developer",
        "build": "cross-env NODE_ENV=production yarpm run gulp-task-build",
        "build:prod": "cross-env NODE_ENV=production yarpm run gulp-task-build",
        "build:stag": "cross-env NODE_ENV=staging yarpm run gulp-task-build",
        "build:beta": "cross-env NODE_ENV=beta yarpm run gulp-task-build",
        "webpack": "webpack-dev-server --progress --colors --hot --open --mode development --config ./src-front/webpack.config.js"
    },
    "license": "Apache-2.0",
    "description": "Boilerplate nodejs + typescript + react",
    "author": {
        "name": "EarthW0rm - Marco Antonio Luqueti Faustino"
    },
    "main": "./build/royalgorillaapp.js",
    "dependencies": {
        "bluebird": "3.5.1",
        "body-parser": "1.18.3",
        "cross-env": "^5.2.0",
        "dotenv": "6.0.0",
        "express": "^4.16.3",
        "jsend": "1.0.2",
        "recursive-readdir": "2.2.2",
        "sequelize": "4.38.0",
        "yarpm": "0.2.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@babel/core": "7.1.2",
        "@babel/plugin-proposal-object-rest-spread": "7.0.0",
        "@babel/plugin-transform-arrow-functions": "7.0.0",
        "@babel/polyfill": "7.0.0",
        "@babel/preset-env": "7.1.0",
        "@babel/preset-react": "7.0.0",
        "@types/bluebird": "3.5.23",
        "@types/body-parser": "1.17.0",
        "@types/express": "4.16.0",
        "@types/express-serve-static-core": "4.16.0",
        "@types/jsend": "1.0.28",
        "@types/mime": "2.0.0",
        "@types/node": "10.9.1",
        "@types/recursive-readdir": "2.2.0",
        "@types/sequelize": "4.27.25",
        "@types/serve-static": "1.13.2",
        "ansi-colors": "^2.0.2",
        "axios": "0.18.0",
        "babel-loader": "8.0.4",
        "bootstrap": "4.1.3",
        "chai": "4.1.2",
        "css-loader": "1.0.0",
        "del": "3.0.0",
        "eslint": "4.19.1",
        "eslint-plugin-react": "^7.11.1",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "2.0.0",
        "gulp": "4.0.0",
        "gulp-mocha": "6.0.0",
        "gulp-nodemon": "^2.2.1",
        "gulp-rename": "1.4.0",
        "gulp-sourcemaps": "2.6.4",
        "gulp-typescript": "5.0.0-alpha.3",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "jsonfile": "^5.0.0",
        "mocha": "5.2.0",
        "node-sass": "4.9.3",
        "nodemon": "1.18.3",
        "ramda": "0.25.0",
        "react": "16.5.2",
        "react-dom": "16.5.2",
        "react-redux": "5.0.7",
        "react-router": "4.3.1",
        "react-router-dom": "4.3.1",
        "reactstrap": "6.5.0",
        "redux": "4.0.0",
        "redux-multi": "0.1.12",
        "redux-promise": "0.6.0",
        "redux-thunk": "2.3.0",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "tslint": "5.11.0",
        "tslint-consistent-codestyle": "1.13.3",
        "tslint-eslint-rules": "5.4.0",
        "tslint-microsoft-contrib": "5.2.0",
        "typescript": "3.0.1",
        "url-loader": "1.1.1",
        "webpack": "4.20.2",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "^3.1.9"
    },
    "peerDependencies": {
        "gulp-util": "^3.0.8",
        "webpack-cli": "3.1.2"
    }
}
